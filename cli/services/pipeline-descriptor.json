{
  "deployment-type": "docker-compose",
  "stages": {
    "ingestion": [
      {
        "storage": {
          "type": "series",
          "protocol": "http",
          "persistence": true
        },
        "polling": true,
        "custom-config": "./cli/configs/prometheus/prometheus.yml",
        "id": "backend1",
        "implementation": "Prometheus"
      },
      {
        "view": {
          "type": "panel",
          "protocol": "http",
          "persistence": true
        },
        "custom-config": "./cli/configs/grafana/grafana.ini",
        "implementation": "Grafana",
        "id": "view1"
      }
    ],
    "processing": [],
    "output": {
      "backend1": {
        "links": "view1"
      }
    }
  }
}